<div class="container-sessions">
  <div class="group-action-from">

    <div class="group-content">
      <p>Already have an account? Connect.</p>
      <!-- Group Links Navigation -->
      <div class="group-navigation-link">
        <!-- log-in -->
        <div class="log-in">
         <%= render 'pages/auth/links/link_logIn' %>
        </div>
        <!-- sign-up -->
        <div class="sign-up">
         <%= render 'pages/auth/links/link_signUp' %>
        </div>
      </div>
      <!-- Form -->
      <%= render 'pages/auth/sessions/form' %>
    </div>
    <!-- Image -->
    <div>
      <%= image_tag "logIn.png", alt: "Log-In", class: "example-class" %>
    </div>
  </div>
</div>
